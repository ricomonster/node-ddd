{
  "name": "node-ddd",
  "version": "2.0.0",
  "description": "Node DDD Boilerplate",
  "engines": {
    "node": ">=14 <15"
  },
  "scripts": {
    "build": "tsc -p tsconfig-build.json",
    "coverage": "jest --coverage",
    "db:migrate:latest": "knex --knexfile ./src/config/database.ts migrate:latest",
    "db:migrate:make": "knex --knexfile ./src/config/database.ts migrate:make -x ts",
    "db:migrate:undo": "knex --knexfile ./src/config/database.ts migrate:rollback",
    "db:migrate:undo:all": "knex --knexfile ./src/config/database.ts migrate:rollback --all",
    "db:migrate:refresh": "npm run db:migrate:undo:all && npm run db:migrate",
    "db:seed": "knex --knexfile ./src/config/database.ts seed:run",
    "db:seed:make": "knex --knexfile ./src/config/database.ts seed:make -x ts",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "precommit": "npm run lint; npm run test",
    "prepare": "husky install",
    "start:sls": "sls offline start --stage local",
    "test": "jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ricomonster/node-ddd.git"
  },
  "author": "Rico Maglayon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ricomonster/node-ddd/issues"
  },
  "homepage": "https://github.com/ricomonster/node-ddd#readme",
  "dependencies": {
    "awilix": "^7.0.3",
    "aws-lambda": "^1.0.7",
    "axios": "^0.27.2",
    "dotenv": "^16.0.0",
    "http-status-codes": "2.2.0",
    "knex": "^2.0.0",
    "objection": "^3.0.1",
    "pg": "^8.7.3",
    "uuid": "^8.3.2",
    "validatorjs": "3.22.1",
    "winston": "3.7.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.95",
    "@types/jest": "27.5.0",
    "@types/node": "17.0.31",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.2",
    "@types/validatorjs": "3.15.0",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "eslint": "8.15.0",
    "eslint-plugin-jsdoc": "^39.2.9",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "7.0.4",
    "jest": "^28.1.0",
    "serverless": "^3.16.0",
    "serverless-offline": "^8.7.0",
    "serverless-plugin-typescript": "^2.1.2",
    "ts-jest": "^28.0.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
